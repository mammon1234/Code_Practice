public void replace(char[] source, int length){
	int spaceCount = 0;
	for(int i = 0; i < length; i++){
		if(source[i] == ' '){
			spaceCount++;
		}
	}
	int newLength = length + 2*spaceCount;
	for(int i = length-1; i >= 0; i--){
		if(soucre[i] == ' '){
			source[newLength -1]='0';
			source[newLength -2]='2';
			source[newLength -3]='%';
			newLength-=3;
		}else{
			source[newLength - 1] = source[i];
			newLength-=1;
		}
	}
}
