public class Solution {
    public int firstUniqChar(String s) {
        if(s == null || s.length() == 0){
            return -1;
        }
        int[] index = new int[128];
        int len = s.length();
        int uni = 0;
        for (int i = 0; i < len; i++){
            char temp=s.charAt(i);
            int ascii = (int) temp;
            if(index[ascii]==0){
                index[ascii] = 1;
            }else if(index[ascii] == 1){
                index[ascii]=-1;
            }
        }
        for (int i = 0; i < len; i++){
            char temp=s.charAt(i);
            int ascii = (int) temp;
            if(index[ascii] == 1){
                return i;
            }
        }
        return -1;
    }
}
